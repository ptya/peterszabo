{
  "name": "peter-szabo-homepage",
  "private": true,
  "description": "My personal homepage",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "lint": "eslint src"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "formik": "^2.0.3",
    "gatsby": "^2.13.31",
    "gatsby-background-image": "^0.8.0",
    "gatsby-image": "^2.2.6",
    "gatsby-plugin-advanced-sitemap": "^1.4.5",
    "gatsby-plugin-manifest": "^2.2.37",
    "gatsby-plugin-offline": "^3.0.31",
    "gatsby-plugin-polyfill-io": "^1.1.0",
    "gatsby-plugin-react-helmet": "^3.1.18",
    "gatsby-plugin-root-import": "^2.0.5",
    "gatsby-plugin-sharp": "^2.2.9",
    "gatsby-plugin-styled-components": "^3.1.2",
    "gatsby-plugin-typescript": "^2.2.0",
    "gatsby-source-filesystem": "^2.1.6",
    "gatsby-transformer-remark": "^2.6.37",
    "gatsby-transformer-sharp": "^2.2.4",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^6.0.0-beta",
    "react-map-gl": "^5.1.5",
    "react-responsive": "^8.0.1",
    "react-spring": "^8.0.27",
    "resize-observer-polyfill": "^1.5.1",
    "styled-components": "^5.0.0",
    "styled-normalize": "^8.0.6",
    "typeface-montserrat": "^0.0.75",
    "typeface-raleway": "^0.0.75"
  },
  "devDependencies": {
    "@types/node": "^13.7.7",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-helmet": "^5.0.15",
    "@types/react-map-gl": "^5.2.1",
    "@types/react-responsive": "^8.0.2",
    "@types/resize-observer-browser": "^0.1.3",
    "@types/styled-components": "^5.0.1",
    "babel-eslint": "10.0.3",
    "babel-plugin-styled-components": "^1.10.6",
    "eslint": "6.6.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.5.0",
    "eslint-config-wesbos": "0.0.19",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-html": "6.0.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "7.16.0",
    "eslint-plugin-react-hooks": "2.2.0",
    "husky": "^4.0.7",
    "prettier": "^2.0.2",
    "typescript": "^3.8.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "eslintConfig": {
    "extends": [
      "wesbos"
    ],
    "settings": {
      "import/resolver": {
        "alias": [
          [
            "components",
            "./src/components"
          ],
          [
            "assets",
            "./src/assets"
          ],
          [
            "pages",
            "./src/pages"
          ],
          [
            "utils",
            "./src/utils"
          ]
        ]
      }
    },
    "rules": {
      "react/jsx-props-no-spreading": "off",
      "jsx-a11y/label-has-associated-control": [
        2,
        {
          "labelComponents": [
            "Label"
          ],
          "labelAttributes": [
            "label"
          ],
          "controlComponents": [
            "Input"
          ],
          "assert": "htmlFor",
          "depth": 3
        }
      ],
      "jsx-a11y/label-has-for": "off"
    }
  }
}
